               call GetMeHome;
               ndebug("Make Me Move 4");
               set_local_var(LVAR_Replace_Bot,1);
           end

           else begin
               Repair_Bot+=1;
               if (Repair_Bot == BOT_REPAIR_RATE) then begin
                   float_msg(self_obj,g_mstr(9002),FLOAT_MSG_GREEN);
