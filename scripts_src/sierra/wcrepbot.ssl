                   call GetMeHome;
               end
           end

       end
       else if (local_var(LVAR_Wait_Time) == 0) then  begin
           set_local_var(LVAR_Wait_Time,game_time);
       end
       else if ((game_time - local_var(LVAR_Wait_Time)) > (2*ONE_GAME_MINUTE)) then begin
           call GetMeHome;
       end
   end

   else begin
       call Travel4;
   end
end

